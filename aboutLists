import easygui as g

shopping_list = [
    "Bread",#0
    "Milk",#1
    "Eggs",#2
    "Oranges",#3
    "Juice",#4
    "Rice",#5
    "Beef",#6
    "Tomatoes"#7
]

#lists have [ ] around them 
#tuples have ( ) around them and items cannot be added/removed, but can still be indexed

g.msgbox(f"I need to buy {shopping_list[2]} and {shopping_list[6]} for dinner tonight.")

shopping_list.append("bananas") #adds the item to the last position of shopping_list

g.msgbox(shopping_list)

shopping_list.pop() #removes the last item in shopping_list

g.msgbox(shopping_list)